<?xml version="1.0" encoding="UTF-8" ?>

<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee web-jsptaglibrary_2_0.xsd"
	version="2.0">
	<description>权限按钮签库</description>
	<tlib-version>1.0</tlib-version>
	<description>A page tab library for the system</description>
	<tag>
		<name>button</name>
		<tag-class>com.symbol.wp.core.tag.ButtonTag</tag-class>
		<body-content>jsp</body-content>
		<attribute>
			<description>按钮css</description>
			<name>className</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>经过按钮css</description>
			<name>onmouseoverClassName</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>移出按钮css</description>
			<name>onmouseoutClassName</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>


